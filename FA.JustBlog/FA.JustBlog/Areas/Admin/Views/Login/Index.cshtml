<body>
    <div class="container">
        <div class="row">
            <div class="col-sm-9 col-md-7 col-lg-5 mx-auto">
                <div class="card card-signin my-5">
                    <div class="card-body">
                        <h5 class="card-title text-center">
                            Đăng nhập trang quản trị
                        </h5>
                        @*<p class="text-muted">Truy cập trang quản trị</p>*@
                        <form class="form-signin">
                            <div class="form-group">
                                <label class="control-label">Email</label>
                                <input class="form-control"
                                       type="text"
                                       placeholder="Nhập địa chỉ email" />
                            </div>
                            <div class="form-group">
                                <label class="control-label">Mật khẩu</label>
                                <input class="form-control"
                                       type="password"
                                       placeholder="Nhập mật khẩu" />
                            </div>
                            <button class="btn btn-lg  btn-block text-uppercase" type="submit">Đăng nhập</button>

                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- jQuery -->
    <script src="~/Areas/Asset/js/jquery-3.5.0.min.js"></script>

    <!-- Bootstrap Core JS -->
    <script src="~/Areas/Asset/js/popper.min.js"></script>
    <script src="~/Areas/Asset/plugins/bootstrap/js/bootstrap.min.js"></script>

